    var w1 = instance_create(w1X, w1Y, obj_workerHumanP1);
    w1.sprite_index = spr_workerIdle;
    w1Draw = 0; 
} 

if (timer == 570){
    var w2 = instance_create(w2X, w2Y, obj_workerHumanP1);
    w2.sprite_index = spr_workerIdle;   
