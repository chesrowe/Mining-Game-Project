        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// World gen

//vars
global.surfaceHeightGrid = ds_grid_create(50000,1000);
global.worldGenSurface = surface_create(room_width,room_height);
global.tileCount = 0;
surfaceHeight = 696;
xPos = 0;
tileIndex = 0;
worldWidth = get_integer("World width in tiles",500); //In tiles (32x32)
hillChance = get_integer("Hillyness (ranges from 1-1000, 1 being super hilly)",10);
hillHeightLimit = get_integer("The soft-max y value that the hills can reach(the lower, the higher the hills)",400);

//Put tiles into arrays
surfaceFlatTile[0] = obj_tileSurfaceFlat1;
surfaceFlatTile[1] = obj_tileSurfaceFlat2;
surfaceFlatTile[2] = obj_tileSurfaceFlat3;
surfaceFlatTile[3] = obj_tileSurfaceFlat4;

//Randomize seed 
randomize();

//Generate surface land 
for (i = 0; i &lt;= worldWidth; i++;){
    //Chance to start creating a hill
    if (irandom(hillChance) == 1){
        if (surfaceHeight &lt;= (696-64)){
            var hillHeight = choose(32,64,-32,-64);
        }else if (surfaceHeight &lt; (hillHeightLimit)){
            var hillHeight = choose(-32,-64,-32,-64,32);
        }else{
            var hillHeight = choose(32,64);
        }
        
        surfaceHeight -= hillHeight;    
        //Add the sloped tiles based on hill height
        switch(hillHeight){
            //Add one slope if the height is 32
            case 32:
                ds_grid_add(global.surfaceHeightGrid,xPos,0,surfaceHeight);
                instance_create(xPos,surfaceHeight,obj_tileSurfaceHillUp);
                instance_create(xPos,surfaceHeight+32,obj_tileSurfaceHillUpUnder);
                //Create one flat tile after slope
                ds_grid_add(global.surfaceHeightGrid,xPos+32,0,surfaceHeight);
                instance_create(xPos+32,surfaceHeight,surfaceFlatTile[tileIndex]);
                tileIndex ++;
                if (tileIndex &gt; 3){
                    tileIndex = choose(0,1,2);
                }
                xPos += 64;
                global.tileCount += 3;
            break;
            
            //Add 2 if the height is 64
            case 64:
                ds_grid_add(global.surfaceHeightGrid,xPos,0,surfaceHeight+32);
                instance_create(xPos,surfaceHeight+32,obj_tileSurfaceHillUp);
                instance_create(xPos,surfaceHeight+64,obj_tileSurfaceHillUpUnder);
                ds_grid_add(global.surfaceHeightGrid,xPos+32,0,surfaceHeight);
                instance_create(xPos+32,surfaceHeight,obj_tileSurfaceHillUp);
                instance_create(xPos+32,surfaceHeight+32,obj_tileSurfaceHillUpUnder);
                //Create one flat tile after slope
                ds_grid_add(global.surfaceHeightGrid,xPos+64,0,surfaceHeight);
                instance_create(xPos+64,surfaceHeight,surfaceFlatTile[tileIndex]);
                tileIndex ++;
                if (tileIndex &gt; 3){
                    tileIndex = choose(0,1,2);
                }
                xPos+= 96;
                global.tileCount += 5;
            break;
            
            //Add one slope if the height is -32
            case -32:
                ds_grid_add(global.surfaceHeightGrid,xPos,0,surfaceHeight-32);
                instance_create(xPos,surfaceHeight-32,obj_tileSurfaceHillDown);
                instance_create(xPos,surfaceHeight,obj_tileSurfaceHillDownUnder);
                //Create one flat tile after slope
                ds_grid_add(global.surfaceHeightGrid,xPos+32,0,surfaceHeight);
                instance_create(xPos+32,surfaceHeight,surfaceFlatTile[tileIndex]);
                tileIndex ++;
                if (tileIndex &gt; 3){
                    tileIndex = choose(0,1,2);
                }
                xPos += 64;
                global.tileCount += 3;
            break;
            
            //Add 2 if the height is -64
            case -64:
                ds_grid_add(global.surfaceHeightGrid,xPos,0,surfaceHeight-64);
                instance_create(xPos,surfaceHeight-64,obj_tileSurfaceHillDown);
                instance_create(xPos,surfaceHeight-32,obj_tileSurfaceHillDownUnder);
                ds_grid_add(global.surfaceHeightGrid,xPos+32,0,surfaceHeight-32);
                instance_create(xPos+32,surfaceHeight-32,obj_tileSurfaceHillDown);
                instance_create(xPos+32,surfaceHeight,obj_tileSurfaceHillDownUnder);
                //Create one flat tile after slope
                ds_grid_add(global.surfaceHeightGrid,xPos+64,0,surfaceHeight);
                instance_create(xPos+64,surfaceHeight,surfaceFlatTile[tileIndex]);
                tileIndex ++;
                if (tileIndex &gt; 3){
                    tileIndex = choose(0,1,2);
                }
                xPos+= 96;
                global.tileCount += 5;
            break;
        }
    }else{
        //Keep creating land
        instance_create(xPos,surfaceHeight,surfaceFlatTile[tileIndex]);
        ds_grid_add(global.surfaceHeightGrid,xPos,0,surfaceHeight);
        global.tileCount += 1;
        xPos += 32;
        tileIndex ++;
        if (tileIndex &gt; 3){
            tileIndex = choose(0,1,2);
        }
        
    }   

}

//Generate Foliage/resources
foliageSpawnList = ds_list_create();
//add foliage/resources to ds list
repeat(15){
    ds_list_add(foliageSpawnList,obj_tree1);
}
repeat(2){
    ds_list_add(foliageSpawnList,obj_stone);
}
repeat(3){
    ds_list_add(foliageSpawnList,obj_oakTree);
}
repeat(1){
    ds_list_add(foliageSpawnList,obj_gold);
}
repeat(5){
    ds_list_add(foliageSpawnList,obj_treeBirch);
}
xPos = 0;
var spawnChance = 2;
//Generate foliage/resources
for (i = 0; i &lt;= worldWidth; i++;){

    //Chance to spawn foliage/resource
    if (irandom(spawnChance) == 1){
        ds_list_shuffle(foliageSpawnList);
        var foliage = ds_list_find_value(foliageSpawnList,0);
        var folY = ds_grid_get(global.surfaceHeightGrid,xPos,0);
        //Place the foliage based on what type it is 
        switch(foliage){
            //spawn common tree
            case obj_tree1: 
                instance_create(xPos,folY-16,foliage);
                xPos += 96;
            break;
            //spawn oak tree
            case obj_oakTree:
                instance_create(xPos+96,folY-16,foliage);
                xPos += 192;
            break;
            //Spawn stone 
            case obj_stone:
                instance_create(xPos,folY-32,foliage);
                xPos += 96;
            break;
            //Spawn gold 
            case obj_gold:
                instance_create(xPos,folY-32,foliage);
                xPos += 96;
            break;
            //Spawn birch tree 
            case obj_treeBirch:
                instance_create(xPos,folY-32,foliage);
                xPos += 96;
            break;
        }
        
    }else{
        xPos += 32;
    }

}
//After world is generated, move camera to center of the room
view_xview[0] = (worldWidth/2)*32;




