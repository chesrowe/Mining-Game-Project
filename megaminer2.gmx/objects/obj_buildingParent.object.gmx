            <string>///General
sprite_index = spriteLevel[(level-1)];

groundY = (instance_nearest(mouse_x,mouse_y,obj_tileSurfaceFlatParent).y);

//Makes the building follow the right curser
if (!global.gamepad){
    placeX = mouse_x;
    placeY = mouse_y
}
//Make the building follow the curcer when it's not placed
if (!placed){
    image_alpha = .5;
    x = placeX-originOffX;
    if (placeY &gt; groundY - 50){
        y = groundY-originOffY;
    }else{
        y = placeY;
    }
}else{
    //place the building (fade in)
    scr_effects_fade_in(.2);
    if (global.selected != id &amp;&amp; menu &amp;&amp; myMenu != noone){
        menu = 0;
        with(myMenu){
            instance_destroy();
        }
        myMenu = noone;
    }
    //play the place sound
    if (!placeSoundPlayed){
        var sound = audio_play_sound_at(placeSound[irandom(3)],x,y,0,1000,200,1,0,999);
        audio_sound_gain(sound,global.sfxVol,0);
        placeSoundPlayed = 1;
    }
}
