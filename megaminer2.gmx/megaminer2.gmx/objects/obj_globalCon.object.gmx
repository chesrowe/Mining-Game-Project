global.controllerCount = gamepad_get_device_count();

if (gamepad_is_connected(0)){
    if (gamepad_button_check_released(0, gp_face1)){
       if (global.selectedP1 == 0 &amp;&amp; global.buildingP1 &amp;&amp; !global.wheatBuildP1 &amp;&amp; global.goldP1 &gt;= 800){
            global.wheatBuildP1 = 1;
            if (!global.wheatCreatedP1){
                lastWheat = instance_create(obj_selectorP1.x,obj_selectorP1.y,obj_wheatFarmP1);
                global.wheatCreatedP1 = 1;   
            }
        }
         if (global.wheatBuildP1 &amp;&amp; global.wheatCreatedP1 &amp;&amp; obj_selectorP1.y == 1024){
            lastWheat.dummy = 0;
            global.goldP1 -= 800;
            global.wheatBuildP1 = 0;
            global.wheatCreatedP1 = 0;    
        }
        if (global.selectedP1 == 1 &amp;&amp; !global.minersP1){
            global.buildingP1 = 1;
        }
       if(global.selectedP1 == 0 &amp;&amp; global.buildingP1 == 0 &amp;&amp; global.minerSelectP1 == 0 &amp;&amp; global.minersP1 == 1){    
            if (global.goldP1 &gt;= 400){ 
                global.minerSelectP1 = 1;
                global.goldP1 -= 400;
            }    
        }
         if (global.selectedP1 == 0 &amp;&amp; !global.buildingP1 &amp;&amp; !global.minersP1){
            global.minersP1 = 1;
            global.buildingP1 = 0;
        }
    }
    if (gamepad_button_check_released(0, gp_padr)){
        if(global.selectedP1 &lt;= 4){
            global.selectedP1 += 1;
        }
    }
     if (gamepad_button_check_released(0, gp_padl)){
        if(global.selectedP1 &gt;= 1){
            global.selectedP1 -= 1;
        }
    }
}

if (makeMinerP1){
    if (!global.minerSelectedP1){
        minerTimerP1 ++;
    }
    if (minerTimerP1 &gt;= 800){
        //makeMinerP1 = 0;
        //minerTimerP1 = 0;
    }
}

if (keyboard_check_released(vk_space)){
    if (!window_get_fullscreen()){
        window_set_fullscreen(true);    
    }else{
        window_set_fullscreen(false);
    }
}

