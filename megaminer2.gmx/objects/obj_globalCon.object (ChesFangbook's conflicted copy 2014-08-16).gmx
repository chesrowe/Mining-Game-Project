            <string>///Deals with main game mechcanics!! MIXED PLAYERS!! not just player1
global.controllerCount = gamepad_get_device_count(); //
//Sloppy jumbled code below --&gt;
if (gamepad_is_connected(0)){ // &lt;-- Checks to see if the gamepad is connected
    if (gamepad_button_check_released(0, gp_face1)){// &lt;-- Checks for the 'A' button to be released!! 
       if (global.selectedP1 == 0 &amp;&amp; global.buildingP1 &amp;&amp; !global.wheatBuildP1 &amp;&amp; global.goldP1 &gt;= 800){ // &lt;-- Start of wheat farm code!
            global.wheatBuildP1 = 1;
            if (!global.wheatCreatedP1){
                lastWheat = instance_create(obj_selectorP1.x,obj_selectorP1.y,obj_wheatFarmP1);
                global.wheatCreatedP1 = 1;   
            }
        }
         if (global.wheatBuildP1 &amp;&amp; global.wheatCreatedP1 &amp;&amp; obj_selectorP1.y == 1024){
            lastWheat.dummy = 0;
            global.goldP1 -= 800;
            global.wheatBuildP1 = 0;
            global.wheatCreatedP1 = 0;    
        }
        if (global.selectedP1 == 1 &amp;&amp; !global.minersP1){
            global.buildingP1 = 1;
        }
       if(global.selectedP1 == 0 &amp;&amp; global.buildingP1 == 0 &amp;&amp; global.minerSelectP1 == 0 &amp;&amp; global.minersP1 == 1){    
            if (global.goldP1 &gt;= 400){ 
                global.minerSelectP1 = 1;
                global.goldP1 -= 400;
            }    
        }
         if (global.selectedP1 == 0 &amp;&amp; !global.buildingP1 &amp;&amp; !global.minersP1){ // &lt;-- If the miner icon is selected, this makes the miner menu show up! 
            global.minersP1 = 1;
            global.buildingP1 = 0;
        }
    } // &lt;-- End of 'A' button check :D \\
    
    if (gamepad_button_check_released(0, gp_padr)){ // &lt;-- controls D-pad menu movement !
        if(global.selectedP1 &lt;= 2){
            global.selectedP1 += 1;
        }
    }
     if (gamepad_button_check_released(0, gp_padl)){
        if(global.selectedP1 &gt;= 1){
            global.selectedP1 -= 1;
        }
    }
} 

if (makeMinerP1){
    if (!global.minerSelectedP1){
        minerTimerP1 ++;
    }
}

if (keyboard_check_released(vk_space)){ // &lt;-- Spacebar makes the game go to full screen!
    if (!window_get_fullscreen()){
        window_set_fullscreen(true);    
    }else{
        window_set_fullscreen(false);
    }
} // &lt;-- End of spacebar full screen code!

if (gamepad_button_check_pressed(0, gp_face1)){
    if (global.minerSelectP1){
        obj_globalCon.minerTimerP1 ++;
        obj_globalCon.makeMinerP1 = 1;
        global.minerSelectP1 = 0;
    }
}

if (obj_globalCon.minerTimerP1 &gt;= 1){
    obj_globalCon.minerTimerP1 ++;
    global.minerSelectP1 = 0;
}

if (global.minerSelectRockP1 == 0 &amp;&amp; obj_globalCon.minerTimerP1 &gt;= 800){
    miner = instance_create(obj_townCenter.x,obj_townCenter.y+115,obj_minerP1);
    with (miner){
        gold = 1;
    }
    obj_globalCon.makeMinerP1 = 0;
    obj_globalCon.minerTimerP1  = 0;
    global.minerSelectP1 = 0;                      
}
if (global.minerSelectRockP1 == 1 &amp;&amp; obj_globalCon.minerTimerP1 &gt;= 800){
    miner = instance_create(obj_townCenter.x,obj_townCenter.y+115,obj_minerP1);
    miner.gold = 0;
    miner.stone = 1;
    with (miner){
        stone = 1;
        gold = 0;
    }
    obj_globalCon.makeMinerP1 = 0;
    obj_globalCon.minerTimerP1  = 0;
    global.minerSelectP1 = 0;           
}

if (gamepad_button_check_pressed(0, gp_face2)){
    if (global.minerSelectP1 &amp;&amp; !obj_globalCon.makeMinerP1){
        global.minerSelectP1 = 0;
        global.goldP1 += 400;
    }
}

if (global.wheatBuildP1){
    y = 1024;
    gamepad_movement(0);
    y = 1024;
}

