    if !(position_meeting(x,y+16,obj_tileSurfaceFlatParent)){
        groundY = (instance_nearest(mouse_x,mouse_y,obj_tileSurfaceFlatParent).y);
    }else{
        groundY = (instance_position(x,y+16,obj_tileSurfaceFlatParent).y);
    }
    placeX = mouse_x;
    placeY = mouse_y
    image_alpha = .5;
