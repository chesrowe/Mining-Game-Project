//Makes the tree fall
if (falling){
    if (!soundPlayed){
        audio_play_sound_at(fallingSound[irandom(3)],x,y,0,1000,200,1,0,9999900);    
        soundPlayed = 1;
    }
    fallIncrement += .001;
    fallTimer --;
    if (fallSpeed &lt; 5){
        fallSpeed += fallIncrement;
    }
    if (fallTimer &lt;= 0){
        falling = 0;
        fell = 1;
    }
    if (fallDir = 'right'){
        image_angle -= fallSpeed;  
        x += .2;  
    }else{
        image_angle += fallSpeed;
        x -= .2;
    }

}
